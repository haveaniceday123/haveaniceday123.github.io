<span class="n">FLOAT</span> <span class="p">{</span><span class="n">HEX_FLOAT</span><span class="p">}</span><span class="o">|</span><span class="p">{</span><span class="n">DECIMAL_FLOAT</span><span class="p">}(</span><span class="n">f</span><span class="o">|</span><span class="n">F</span><span class="o">|</span><span class="n">l</span><span class="o">|</span><span class="n">L</span><span class="p">)</span><span class="o">?</span>

<span class="o">%%</span>

<span class="p">{</span><span class="n">COMMENT</span><span class="p">}</span><span class="o">|</span><span class="p">{</span><span class="n">SINGLE_COMMENT</span><span class="p">}</span><span class="o">|</span><span class="p">{</span><span class="n">IDENTIFIER</span><span class="p">}</span><span class="o">|</span><span class="p">{</span><span class="n">NEW_LINE</span><span class="p">}</span> <span class="p">;</span>
<span class="p">{</span><span class="n">INTEGER</span><span class="p">}</span> <span class="n">printf</span><span class="p">(</span><span class="s">"No.%d integer: %s</span><span class="se">\n</span><span class="s">"</span><span class="p">,</span> <span class="o">++</span><span class="n">cnt_integer</span><span class="p">,</span> <span class="n">yytext</span><span class="p">);</span>
<span class="p">{</span><span class="n">FLOAT</span><span class="p">}</span> <span class="n">printf</span><span class="p">(</span><span class="s">"No.%d float: %s</span><span class="se">\n</span><span class="s">"</span><span class="p">,</span> <span class="o">++</span><span class="n">cnt_float</span><span class="p">,</span> <span class="n">yytext</span><span class="p">);</span>
<span class="p">.</span> <span class="p">;</span>

<span class="o">%%</span>

<span class="kt">int</span> <span class="nf">yywrap</span><span class="p">()</span> <span class="p">{}</span>
<span class="kt">int</span> <span class="nf">main</span><span class="p">()</span>
<span class="p">{</span>
    <span class="kt">float</span> <span class="err">\</span><span class="n">U0001f431</span> <span class="o">=</span> <span class="o">+</span><span class="mf">31.415926E-1</span><span class="n">F</span><span class="p">;</span>
    <span class="cm">/*
    3.14
    */</span>
    <span class="c1">//3.14</span>
    <span class="n">yylex</span><span class="p">();</span>
    <span class="n">printf</span><span class="p">(</span><span class="s">"</span><span class="se">\n</span><span class="s">%d integer(s), %d float(s) find."</span><span class="p">,</span> <span class="n">cnt_integer</span><span class="p">,</span> <span class="n">cnt_float</span><span class="p">);</span>
<span class="p">}</span>
</code></pre></div></div>


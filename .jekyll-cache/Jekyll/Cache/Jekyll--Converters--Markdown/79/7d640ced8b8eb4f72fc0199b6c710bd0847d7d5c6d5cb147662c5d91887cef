 * @see yy_switch_to_buffer
 */</span>
<span class="kt">void</span> <span class="nf">yyset_in</span> <span class="p">(</span><span class="kt">FILE</span> <span class="o">*</span>  <span class="n">_in_str</span> <span class="p">)</span>

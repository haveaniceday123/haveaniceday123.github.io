			<span class="k">if</span> <span class="p">(</span><span class="n">a</span><span class="p">[</span><span class="n">j</span><span class="p">]</span> <span class="o">==</span> <span class="sc">'A'</span> <span class="o">+</span> <span class="n">i</span><span class="p">)</span>
